{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": [
        "rds-db:connect"
      ],
      "Effect": "Allow",
      "Resource": "arn:aws:rds:us-east-1:312293704956:db:praktikos-auora-us-east-1b"
    },
    {
      "Sid": "DenyDelete1",
      "Effect": "Deny",
      "Action": "rds:*",
      "Resource": "arn:aws:rds:us-east-1:312293704956:db:praktikos-auora-us-east-1b"
    },
    {
      "Sid": "AllowGroupToPutObjectInBucket",
      "Action": [
        "s3:*",
        "s3:GetObject"
      ],
      "Effect": "Allow",
      "Resource": [
        "arn:aws:s3:::media-*",
        "arn:aws:s3:::media-*/*"
      ]
    },
    {
      "Action": [
        "cognito-idp:ListUserPools"
      ],
      "Effect": "Allow",
      "Resource": "*"
    },
    {
      "Action": [
        "cognito-identity:ListIdentityPools"
      ],
      "Effect": "Allow",
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "kms:Decrypt"
      ],
      "Resource": [
        "*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:DescribeLogStreams",
        "logs:PutLogEvents"
      ],
      "Resource": [
        "*"
      ]
    }
  ]
}
