{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": [
        "cognito-idp:AdminGetUser"
      ],
      "Effect": "Allow",
      "Resource": "*"
    },
    {
      "Action": [
        "cognito-idp:ListUserPools",
        "cognito-idp:AdminGetUser"
      ],
      "Effect": "Allow",
      "Resource": "*"
    },
    {
      "Action": [
        "cognito-identity:ListIdentityPools"
      ],
      "Effect": "Allow",
      "Resource": "*"
    },
    {
      "Sid": "AWSConfigSNSPolicy20150201",
      "Action": [
        "SNS:ListTopics",
        "SNS:GetTopicAttributes",
        "SNS:CreateTopic",
        "SNS:Subscribe",
        "SNS:DeleteTopic",
        "SNS:*"
      ],
      "Effect": "Allow",
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "kms:Decrypt"
      ],
      "Resource": [
        "*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:DescribeLogStreams",
        "logs:PutLogEvents"
      ],
      "Resource": [
        "*"
      ]
    }
  ]
}
