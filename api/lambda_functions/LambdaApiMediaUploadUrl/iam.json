{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowGroupToPutObjectInBucket",
      "Action": [
        "s3:*",
        "s3:GetObject",
        "s3:PutObject"
      ],
      "Effect": "Allow",
      "Resource": [
        "arn:aws:s3:::praktikos-media",
        "arn:aws:s3:::praktikos-media/*"
      ]
    },
    {
      "Action": [
        "cognito-idp:ListUserPools"
      ],
      "Effect": "Allow",
      "Resource": "*"
    },
    {
      "Action": [
        "cognito-identity:ListIdentityPools"
      ],
      "Effect": "Allow",
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "kms:Decrypt"
      ],
      "Resource": [
        "*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:DescribeLogStreams",
        "logs:PutLogEvents"
      ],
      "Resource": [
        "*"
      ]
    }
  ]
}
